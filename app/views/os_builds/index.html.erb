<p id="notice"><%= notice %></p>

<h1>Os Builds</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Price</th>
      <th>Description</th>
      <th>Sleep</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @os_builds.each do |os_build| %>
      <tr>
        <td><%= os_build.name %></td>
        <td><%= os_build.price %></td>
        <td><%= os_build.description %></td>
        <td><%= os_build.sleep %></td>
        <td><%= link_to 'Show', os_build %></td>
        <td><%= link_to 'Edit', edit_os_build_path(os_build) %></td>
        <td><%= link_to 'Destroy', os_build, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Os Build', new_os_build_path %>
